<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.demo2.Request4Controller">
    <Label text="Введите номер комнаты" />
    <TextField fx:id="roomNumberField" promptText="Номер комнаты" />
    <Label text="Введите версию операционной системы" />
    <TextField fx:id="osVersionField" promptText="Версия ОС" />
    <Button text="Выполнить запрос" onAction="#executeRequest" />
</VBox>

